<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>fr.imag.adele.cadse.runtime</groupId>
  <artifactId>fr.imag.adele.cadse.test.runtime</artifactId>
  <packaging>bundle</packaging>
  <name>fr.imag.adele.cadse.test.runtime</name>
  <version>2.3.0-SNAPSHOT</version>
  <parent>
  	<artifactId>fr.imag.adele.cadse.parent.osgi</artifactId>
  	<groupId>fr.imag.adele.cadse</groupId>
  	<version>2.3.2</version>
  </parent>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Export-Package>fr.imag.adele.cadse.test.runtime*;version="${version.cadse}"</Export-Package>
                        <Import-Package>junit.framework*;version="4.8.1",
 						org.junit.*;version="4.8.1",*</Import-Package>
 						<Bundle-Activator>fr.imag.adele.cadse.test.runtime.Activator</Bundle-Activator>
                   </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
    	<dependency>
    		<groupId>fr.imag.adele.cadse</groupId>
    		<artifactId>as.cu</artifactId>
    		<version>2.3.5-SNAPSHOT</version>
    	</dependency>
    	<dependency>
    		<groupId>fr.imag.adele.cadse.runtime</groupId>
    		<artifactId>Model.Workspace.sam.core</artifactId>
    		<version>2.3.0-SNAPSHOT</version>
    	</dependency>
    	<dependency>
    		<groupId>fr.imag.adele.cadse.runtime</groupId>
    		<artifactId>Model.Workspace.sam.composite</artifactId>
    		<version>2.3.0-SNAPSHOT</version>
    	</dependency>
    	<dependency>
    		<groupId>junit</groupId>
    		<artifactId>junit</artifactId>
    		<version>4.8.1</version>
    		<type>jar</type>
    	</dependency>
    </dependencies>
</project>
